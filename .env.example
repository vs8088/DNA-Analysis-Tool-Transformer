# Optional environment configuration for offline / cached usage
# Set to the folder where you downloaded the model (e.g., models/nt-500m)
MODEL_PATH=C:\Users\&USERNAME%\DNA-Analysis-Tool-Transformer\models\nt-500m

# Force local files only; set to 1/true/yes to avoid any downloads
LOCAL_ONLY=1

# Speed up Hugging Face downloads (used when local_only is off)
HF_HUB_ENABLE_HF_TRANSFER=1
HF_HUB_DOWNLOAD_TIMEOUT=1800